<SCRIPT>

// Event handler for window.onload()
function initialize()   {
    initWindow();
    initImages();
    document.body.style.visibility = "visible";
}

// Applies initial state settings for the application
function initWindow()   {
    // delay the call and avoid 'Access denied' exception
    setTimeout(setSize1, 0);
}

function setSize1(){
    setWindowSize(725, 585);
}

function setSize2(){
    setWindowSize(725, document.body.scrollHeight + 25);
}

// Preloads images with a hoverSrc property into an array to improve response time
function initImages()    {
    document.imageArray = new Array();

    if(! document.imageArray)   {
        document.imageArray = new Array();
    }
    
    for(image in document.images)   {
        if(document.getElementById(image.toString()))   {
            image = document.getElementById(image.toString());
            
            if(image.hoverSrc != undefined)  {
                var img = new Image;
                img.src = image.hoverSrc;
                document.imageArray.push(img);
            }
        }
        
    }
    
    // Initialize all images and related elements to their default "normal" state
    resetHoverEffects();

    // delay the call and avoid 'Access denied' exception
    setTimeout(setSize2, 0);
}

// Opens a new window
function popWindow(url, name)
{
	// New window will be used to popup window if we haven't opened a readme before;
	// Otherwise the same window will open again, only toolbar visible, resizable 
	var wndPopup = window.open(url,name,"");
	// Bring new window to foreground
	if(wndPopup) wndPopup.focus();
}

// Resizes the current window
function setWindowSize(width, height){
    window.resizeTo(width, height);
}

function showHoverEffects(id)   {
    // Hide everything
    resetHoverEffects();
    
    // Swap in hover image
    var image = document.getElementById(id);
    if(image.hoverSrc != undefined)    {
        image.src = image.hoverSrc;
    }
    
    // Show related elements
    if(image.objectRef != undefined)    {
        var teaser = document.getElementById(image.objectRef);
        if(teaser) teaser.style.display = "inline";
    }
}

function resetHoverEffects()   {
    for(image in document.images)   {
        if(document.getElementById(image.toString()))   {
            image = document.getElementById(image.toString());
            
            // Reset each image state to normal
            if(image.normalSrc != undefined)  {
                image.src = image.normalSrc;
            }

            // Hide the related elements for each image
            if(image.objectRef != undefined)    {
                document.getElementById(image.objectRef).style.display = "none";
            }
        }
    }
}

function launch(program){
(new ActiveXObject('WScript.Shell')).Run(program);void(0);
}

function handleLaunchErr(msg,url,l)
{
    alert(msg);
    return true;
}

// Immediate execution
//

</SCRIPT>
 
